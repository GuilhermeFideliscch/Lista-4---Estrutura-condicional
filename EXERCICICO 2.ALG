Algoritmo "Exercicico 2"

Var
 valor:real



Inicio
      escreval("digite um numero")
      leia(valor)
      se valor < 0 entao
      escreval("valor negativo")
      senao
       se valor > 0 entao
      escreval("valor positivo")
      senao
      escreval("Zero")
      fimse
      fimse


Fimalgoritmo